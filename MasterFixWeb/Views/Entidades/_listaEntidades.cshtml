@model IEnumerable<MasterFixWeb.Models.Entidade>

<div name="_partialEntidade">
<table class="table table-hover table-bordered table-striped table-responsive" style="max-height:500px;">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CnpjCpf)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InscRg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Uf)
            </th>
            <th>
                Telefone
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr onclick="modal(@Html.DisplayFor(modelItem => item.Id))">
                <td class="col-lg-1" style="white-space:nowrap; overflow:hidden; text-overflow: ellipsis; max-width:250px;">
                    @Html.DisplayFor(modelItem => item.Nome)
                </td>
                <td class="col-lg-1" style="white-space:nowrap; overflow:hidden; text-overflow:clip">
                    @Html.DisplayFor(modelItem => item.CnpjCpf)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.InscRg)
                </td>
                <td class="col-lg-1" style="white-space:nowrap; overflow:hidden; text-overflow:clip">
                    @Html.DisplayFor(modelItem => item.Cidade)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Uf)
                </td>
                <td class="col-lg-1" style="white-space:nowrap; overflow:hidden; text-overflow:clip">
                    @Html.DisplayFor(modelItem => item.Fone1)
                </td>
            </tr>
        }
    </tbody>
</table>
</div>