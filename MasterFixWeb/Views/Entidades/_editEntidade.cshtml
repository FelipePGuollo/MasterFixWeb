@model MasterFixWeb.Models.Entidade

<div class="modal fade" id="EntidadeModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Entidade</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <ul class="nav nav-tabs" id="myTab" role="tablist">
                      <li class="nav-item">
    <a class="nav-link active" id="home-tab" data-toggle="tab" href="#editForm" role="tab" aria-controls="editForm" aria-selected="true">Home</a>
  </li>
  </ul>  
  <div class="tab-content" id="myTabContent">
            <form id="editForm" class="border-left border-right border-bottom tab-pane fade show active" role="tabpanel" aria-labelledby="home-tab" asp-action="Edit">

            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-row">
                <div class="form-group col-md-4">
                <label asp-for="Nome" class="control-label">Nome</label>
                <input asp-for="Nome" id="Nome" class="form-control form-control-sm" />
                <span asp-validation-for="Nome" class="text-danger"></span>
                </div>
                <div class="form-group col-md-2">
                <label asp-for="InscRg" class="control-label">Inscrição Estadual - Rg</label>
                <input asp-for="InscRg" class="form-control form-control-sm" />
                <span asp-validation-for="InscRg" class="text-danger"></span>
                </div>
                <div class="form-group col-md-2">
                <label asp-for="CnpjCpf" class="control-label">Cnpj - Cpf</label>
                <input asp-for="CnpjCpf" class="form-control form-control-sm" />
                <span asp-validation-for="CnpjCpf" class="text-danger"></span>
                </div>
                <div class="form-group col-md-4">
                <label asp-for="Email" class="control-label">Email</label>
                <input asp-for="Email" class="form-control form-control-sm" />
                <span asp-validation-for="Email" class="text-danger"></span>
                </div>
            </div>
            <div class="card">
        <div class="card-header fas fa-angle-down" data-toggle="collapse" data-target="#collapseOne">
        </div>
             <div id="collapseOne" class="collapse">
            <div class="form-row">
                <div class="form-group col-md-2">
                    <label class="control-label">País</label>
                    <input class="form-control form-control-sm" />
                    <span class="text-danger"></span>
                </div>
                <div class="form-group col-md-2">
                    <label asp-for="Cidade" class="control-label">Cidade</label>
                    <input asp-for="Cidade" class="form-control form-control-sm" />
                    <span asp-validation-for="Cidade" class="text-danger"></span>
                </div>
                <div class="form-group col-md-1">
                    <label asp-for="Uf" class="control-label">Uf</label>
                    <input asp-for="Uf" class="form-control form-control-sm" />
                    <span asp-validation-for="Uf" class="text-danger"></span>
                </div>

                <div class="form-group col-md-2">
                    <label asp-for="Bairro" class="control-label">Bairro</label>
                    <input asp-for="Bairro" class="form-control form-control-sm" />
                    <span asp-validation-for="Bairro" class="text-danger"></span>
                </div>
            <div class="form-group col-md-3">
                <label asp-for="Cep" class="control-label">CEP</label>
                <input asp-for="Cep" class="form-control form-control-sm" />
                <span asp-validation-for="Cep" class="text-danger"></span>
            </div>
            </div>

            <div class="form-row">
                <div class="form-group col-md-4">
                    <label asp-for="Rua" class="control-label">Rua</label>
                    <input asp-for="Rua" class="form-control form-control-sm" />
                    <span asp-validation-for="Rua" class="text-danger"></span>
                </div>
                <div class="form-group col-md-1">
                    <label asp-for="Numero" class="control-label">Numero</label>
                    <input asp-for="Numero" class="form-control form-control-sm" />
                    <span asp-validation-for="Numero" class="text-danger"></span>
                </div>
            <div class="form-group col-md-3">
                <label asp-for="Complemento" class="control-label">Complemento</label>
                <input asp-for="Complemento" class="form-control form-control-sm" />
                <span asp-validation-for="Complemento" class="text-danger"></span>
            </div>
            </div>
            </div>
            </div>

            <div class="form-row">
            <div class="form-group col-md-2">
                <label asp-for="Fone1" class="control-label">Fone</label>
                <input asp-for="Fone1" class="form-control form-control-sm" />
                <span asp-validation-for="Fone1" class="text-danger"></span>
            </div>
            <div class="form-group col-md-2">
                <label asp-for="Fone2" class="control-label">Fone</label>
                <input asp-for="Fone2" class="form-control form-control-sm" />
                <span asp-validation-for="Fone2" class="text-danger"></span>
            </div>
            <div class="form-group col-md-2">
                <label asp-for="Contato" class="control-label">Contato</label>
                <input asp-for="Contato" class="form-control form-control-sm" />
                <span asp-validation-for="Contato" class="text-danger"></span>
            </div>
            <div class="form-group col-md-2">
                <label asp-for="ContatoFone" class="control-label">Contato Fone</label>
                <input asp-for="ContatoFone" class="form-control form-control-sm" />
                <span asp-validation-for="ContatoFone" class="text-danger"></span>
            </div>
            <div class="form-group col-md-4">
                <label asp-for="Homepage" class="control-label">Homepage</label>
                <input asp-for="Homepage" class="form-control form-control-sm" />
                <span asp-validation-for="Homepage" class="text-danger"></span>
            </div>
            </div>

            <div class="form-group">
                <label asp-for="getObs" class="control-label">Obs</label>
                <input asp-for="getObs" class="form-control form-control-sm" />
                <span asp-validation-for="getObs" class="text-danger"></span>
            </div>
            </form>
            </div>
            <br>
                <input type="button" onclick="edit()" value="Save" class="btn btn-success">
                <input type="button" value="Close" class="btn btn-secondary" data-dismiss="modal">
                <input onclick="del()" value="Delete" type="button" class="btn btn-danger">
            </div>
        </div>
    </div>
    </div>